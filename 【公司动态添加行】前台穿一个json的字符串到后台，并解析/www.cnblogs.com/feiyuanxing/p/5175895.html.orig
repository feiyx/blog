<p>				$('#btn_addUpload').bind('click', addUpload);<br />				$('#removeUpload').bind('click', removeUpload);<br />				$('#btn_save').bind('click', save);<br />			&lt;/script&gt;<br /> &lt;/body&gt;<br />&lt;/html&gt;</p>
